{
  "name": "@usy-ui/themes",
  "version": "1.0.0",
  "description": "A React UI components library",
  "module": "dist",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "index.ts",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint src --ext .ts,.tsx,.js --fix",
    "format": "prettier src/**/*.{ts,tsx,js,json} --write",
    "test": "jest --runInBand --passWithNoTests",
    "test:cov": "jest --coverage --runInBand",
    "icon:gen": "svgr --icon 20px --title-prop --typescript --replace-attr-values #fff=currentColor --index-template .svgr-template.js --out-dir src/components/Icon src/assets/icons",
    "icon:build": "rimraf src/components/Icon/* --glob && pnpm icon:gen",
    "css:merge": "find src/components -type f -name '*.scss' -exec cat {} ; > src/styles.scss",
    "css:build": "pnpm css:merge && postcss src/styles.scss -o dist/styles.css",
    "build": "rimraf dist && pnpm css:build && rollup -c --bundleConfigAsCjs",
    "build:pack": "pnpm build && pnpm pack",
    "release": "pnpm build && pnpm publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@chromatic-com/storybook": "1.6.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-onboarding": "^8.2.8",
    "@storybook/addon-styling-webpack": "1.0.0",
    "@storybook/addon-webpack5-compiler-babel": "3.0.3",
    "@storybook/blocks": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/react-webpack5": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@svgr/cli": "^8.1.0",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.1",
    "cssnano": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "prop-types": "15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.17.2",
    "rollup-plugin-bundle-stats": "^4.12.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "storybook": "^8.2.8",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "whatwg-fetch": "^3.6.20"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
